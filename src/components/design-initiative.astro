import Layer2 from '../assets/images/DesignInnitiative/2_Skyscraper_front.svg'; 
import Layer3 from '../assets/images/DesignInnitiative/3_Skyscraper_back.svg'; 
import Layer4 from '../assets/images/DesignInnitiative/4_Dark mountains.svg';
import Layer5 from '../assets/images/DesignInnitiative/5_Mid mountains.svg';  
import Layer6 from '../assets/images/DesignInnitiative/6_Pale mountains.svg';  
import Layer7 from '../assets/images/DesignInnitiative/7_white mountain.png';  
import Layer8 from '../assets/images/DesignInnitiative/8_Sky.svg';  

import "../styles/design-initiative.css"
---

<section
    class = "sectionStyle"
>
    <header 
        class="header"
    >
        <img src={Layer1} alt = "layer1" class ="layer1Style" />
        <img src={Layer2} alt = "layer2" class ="layer2Style" />
        <img src={Layer3} alt = "layer3" class ="layer3Style" />
        <img src={Layer4} alt = "layer4" class ="layer4Style" />
        <img src={Layer5} alt = "layer5" class ="layer5Style" />
        <img src={Layer6} alt = "layer6" class ="layer6Style" />
        <img src={Layer7} alt = "layer5" class ="layer7Style" />
        <img src={Layer8} alt = "layer5" class ="layer8Style" />
        <div
            class = "grid translate-y-[-250px]"
        >
            <h1
                class="text-center text-5xl font-bold text-[#fff] textStyle"
            >Command attention. </h1>
            <h1
                class="text-center text-4xl font-bold text-[#000] textStyle"
            >Leave a lasting impression.</h1>
            <p
                class="text-center font-bold text-slate-800 text-2xl"
            >Web development for small to mid-size businesses.</p>
            <button
                class = "rounded-lg bg-[#eb3553] text-2xl font-bold active:translate-x-1 active:translate-y-1 w-fit mx-auto text-white px-1 my-5"
            >Get in touch</button>
        </div>
        
    </header>
